{
  "name": "remix-supabase-saas-kit",
  "version": "0.3.12",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "test:remix": "dotenv -e .env.test npm run dev",
    "supabase:start": "supabase start --ignore-health-check",
    "supabase:stop": "supabase stop",
    "supabase:db:reset": "supabase db reset",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i ./app/styles/index.css -o app/styles/dist.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "stripe:listen": "docker run --rm -it --name=stripe -v ~/.config/stripe:/root/.config/stripe stripe/stripe-cli:latest listen --forward-to http://host.docker.internal:3000/resources/stripe/webhook",
    "stripe:mock-server": "docker run --rm -it -p 12111-12112:12111-12112 stripe/stripe-mock:latest",
    "typegen": "supabase gen types typescript --local > app/database.types.ts",
    "test": "vitest",
    "test:e2e": "sh ./scripts/test.sh",
    "test:db": "supabase test db --debug",
    "test:reset:db": "supabase db reset && supabase test db --debug",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "printWidth": 80,
    "singleQuote": true
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^2.0.16",
    "@radix-ui/react-avatar": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.3",
    "@radix-ui/react-popover": "1.0.4",
    "@radix-ui/react-select": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.0.4",
    "@react-email/body": "^0.0.1",
    "@react-email/button": "^0.0.6",
    "@react-email/column": "^0.0.6",
    "@react-email/head": "^0.0.4",
    "@react-email/html": "^0.0.4",
    "@react-email/preview": "^0.0.5",
    "@react-email/render": "^0.0.6",
    "@react-email/section": "^0.0.8",
    "@react-email/text": "^0.0.4",
    "@remix-run/node": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "@remix-run/serve": "^1.14.0",
    "@remix-run/server-runtime": "^1.14.0",
    "@sentry/node": "^7.40.0",
    "@sentry/react": "^7.40.0",
    "@sentry/remix": "^7.42.0",
    "@sentry/tracing": "^7.40.0",
    "@supabase/auth-helpers-remix": "^0.1.4",
    "@supabase/supabase-js": "^2.10.0",
    "@tanstack/react-query": "^4.24.10",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "csrf": "^3.1.0",
    "cva": "npm:class-variance-authority@^0.4.0",
    "date-fns": "^2.29.3",
    "heroicons": "^2.0.16",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.1.1",
    "isbot": "^3.6.6",
    "nodemailer": "^6.9.1",
    "pino": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.3",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.2.0",
    "react-top-loading-bar": "^2.3.1",
    "recharts": "^2.4.3",
    "remix-i18next": "^5.1.0",
    "stripe": "^11.13.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.0",
    "@remix-run/eslint-config": "^1.14.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.21.1",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/node": "^18.14.5",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "autoprefixer": "^10.4.11",
    "binode": "^1.0.5",
    "c8": "^7.13.0",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "happy-dom": "^8.9.0",
    "msw": "^0.49.3",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.4.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "start-server-and-test": "^2.0.0",
    "supabase": "^1.41.3",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2"
  },
  "engines": {
    "node": ">=14"
  }
}
